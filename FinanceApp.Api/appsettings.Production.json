{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "FinanceApp",
    "Audience": "FinanceAppClient",
    "Key": "default-jwt-key-will-be-overridden-by-env-var",
    "ExpiryMinutes": 60
  },
  "Cors": {
    "AllowedOrigins": [
      "*"
    ]
  },
  "Hangfire": {
    "Queue": "default"
  }
}
